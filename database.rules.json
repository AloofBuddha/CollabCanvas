{
  "rules": {
    "cursors": {
      ".read": "auth != null",
      "$userId": {
        ".write": "auth != null && $userId === auth.uid"
      }
    },
    "presence": {
      ".read": "auth != null",
      "$userId": {
        ".write": "auth != null && $userId === auth.uid"
      }
    },
    "shapes": {
      ".read": "auth != null",
      "$shapeId": {
        ".write": "auth != null"
      }
    }
  }
}

